<?xml version="1.0" encoding="utf-8"?>

<!--Toolbar布局-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/background"
    tools:context=".OneFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/utility_time"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="@drawable/main2_background"
            android:gravity="center"
            android:textSize="120dp"
            android:textColor="#ffffff"/>
        <ImageView
            android:id="@+id/main3"
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:background="@drawable/main3"/>
        <ImageView
            android:id="@+id/main4"
            android:layout_width="match_parent"
            android:layout_height="7dp"
            android:background="@drawable/main4"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="105dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/main5_background_11"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:background="@drawable/main5_background_11"/>
                <TextView
                    android:id="@+id/main5_background_12"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:background="@drawable/main5_background_12"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:textColor="#2DC9D7"/>
                <ImageView
                    android:id="@+id/main5_background_13"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/main5_background_13"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/main5_background_2"
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:background="@drawable/main5_background_2"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/main5_background_31"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:background="@drawable/main5_background_31"/>
                <TextView
                    android:id="@+id/main5_background_32"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:background="@drawable/main5_background_32"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:textColor="#2DC9D7"/>
                <ImageView
                    android:id="@+id/main5_background_33"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/main5_background_33"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/main5_background_4"
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:background="@drawable/main5_background_4"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/main5_background_51"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:background="@drawable/main5_background_51"/>
                <TextView
                    android:id="@+id/main5_background_52"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:background="@drawable/main5_background_52"
                    android:gravity="center"
                    android:textSize="25dp"
                    android:textColor="#2DC9D7"/>
                <ImageView
                    android:id="@+id/main5_background_53"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/main5_background_53"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/main5_background_6"
                android:layout_width="10dp"
                android:layout_height="wrap_content"
                android:background="@drawable/main5_background_6"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/main6_background_1"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="35dp"
                android:background="@drawable/main6_background_1"/>
            <TextView
                android:id="@+id/main6_background_2"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="35dp"
                android:background="@drawable/main6_background_2"
                android:gravity="center"
                android:textSize="18dp"
                android:textColor="#ffffff"/>
            <ImageView
                android:id="@+id/main6_background_3"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_height="35dp"
                android:background="@drawable/main6_background_3"/>
        </LinearLayout>

        <WebView
            android:id="@+id/wv_fanChart"
            android:background="@drawable/background"
            android:layout_width="match_parent"
            android:layout_height="150dp"/>
        <ImageButton
            android:id="@+id/main8"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/main8"
            android:paddingBottom="150dp"/>
        <ImageView
            android:id="@+id/main9"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/main9"/>
    </LinearLayout>

</LinearLayout>